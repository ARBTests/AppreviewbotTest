{
  "dependencies": {
    "puppeteer": "^21.10.0"
  },
  "scripts": {
    "start": "node test/arbSearch.js",
    "test": "jest"
  },
  "jest": 
   
    {
      "testEnvironment": "node",
      "setupFilesAfterEnv": ["<rootDir>/jest.setup.js"],
      "reporters": [
        "default",
        ["jest-html-reporter", {
