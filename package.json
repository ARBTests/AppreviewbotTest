      ],
      "collectCoverage": true,
      "coverageReporters": ["json", "lcov", "text", "clover"]
    
  },
  "transform": {
    "^.+\\.js$": "babel-jest"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "jest-html-reporter": "^3.10.2"
  }
}